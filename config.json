{
  "discord": {
    "base_url": "https://discord.com/api/",
    "notification_path": "webhooks/1286365917332246558/Quvl1pipifkTw5e-SpfA6oNvSuPt67_dRoarGP0YLLr33yKLWEarhJlXXvBrYPl1FEpf"
  },
  "sentry": {
    "traces_sample_rate": 0.0,
    "sample_rate": 1.0,
    "profiles_sample_rate": 0.0
  },
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "default": {
        "format": "%(asctime)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "default",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "INFO",
        "formatter": "default",
        "filename": "app.log",
        "maxBytes": 5242880,
        "backupCount": 3,
        "encoding": "utf8"
      },
      "error_handler": {
        "class": "logger.handler.AsyncErrorHandler",
        "level": "ERROR",
        "formatter": "default",
        "funcs": ["services.discord.asend_notification"]
      }
    },
    "loggers": {
      "": {
        "level": "DEBUG",
        "handlers": [
          "console",
          "file",
          "error_handler"
        ]
      }
    }
  }
